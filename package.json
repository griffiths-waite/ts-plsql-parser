{
  "name": "@griffithswaite/ts-plsql-parser",
  "version": "1.0.2",
  "description": "PL/SQL parser written in TypeScript. Generated using Antlr4.",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js"
    },
    "./*": "./build/*"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "build:antlr4-parser": "antlr4 -o src/antlr -Dlanguage=TypeScript antlr/PlSqlParser.g4 -visitor",
    "build:antlr4-lexer": "antlr4 -o src/antlr -Dlanguage=TypeScript antlr/PlSqlLexer.g4",
    "build:antlr4": "pnpm run build:antlr4-parser && pnpm run build:antlr4-lexer"
  },
  "keywords": [
    "plsql",
    "parser",
    "oracle",
    "antlr4"
  ],
  "author": "warren.bickley@griffiths-waite.co.uk",
  "license": "BSD-3-Clause",
  "dependencies": {
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "antlr4": "4.13.0",
    "@types/node": "^20.8.2",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "antlr4": "4.13.0"
  }
}
